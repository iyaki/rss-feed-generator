{
	"feedPath": "../docs/curated/rss.xml",
	"title": "Recopilador de contenidos curados | iyaki",
	"feed_url": "https://iyaki.ar/curated/rss.xml",
	"site_url": "https://iyaki.ar/curated",
	"image_url": "https://iyaki.ar/images-shared/logo.svg",
	"webMaster": "Ivan Yaki",
	"language": "es",
	"categories": [
		"Tecnología",
		"Diseño de sistemas",
		"Management"
	],
	"ttl": "60",
	"articles": {
		"htmlPath": "../docs/curated/index.html",
		"limit": 20,
		"articlesSelector": "body > main > section > article",
		"title": {
			"selector": "header h1",
			"property": "text"
		},
		"description": {
			"selector": "p:first-of-type",
			"property": "text"
		},
		"url": {
			"selector": "p:last-of-type a",
			"method": "getAttribute",
			"methodArguments": ["href"]
		}
	}
}
